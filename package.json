{
  "name": "fleet-management-system",
  "version": "1.0.0",
  "description": "A comprehensive fleet management system for vehicle tracking, maintenance, and fuel management",
  "main": "api/index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "debug": "nodemon --inspect src/server.js",
    "build": "npx prisma generate",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "generate": "npx prisma generate",
    "seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "db:setup": "npm run migrate && npm run generate && npm run seed",
    "prepare": "npm run generate",
    "studio": "npx prisma studio"
  },
  "keywords": [
    "fleet-management",
    "vehicles",
    "maintenance",
    "fuel-tracking",
    "node",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "pdfkit": "^0.17.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "eslint": "^8.44.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prisma": "^5.22.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": "18.x"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node prisma/seed.js"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  }
}
